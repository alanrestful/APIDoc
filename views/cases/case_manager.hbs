{{> header}}
<style type="text/css">
.body-main{ background-color: #f0f4fb;}
</style>
<div class="container-fluid">
  <div class="row">
    <div class="case-manager">
      <div class="col-sm-3 case-left">
        <div class="setting">
          <div class="setting-header">
            <div class="setting-header-left">设置</div>
            <div class="setting-header-right edit-setting cursor"><i class="iconfont icon-bianji" style="font-size: 12px;"></i>编辑</div>
          </div>
          <div class="setting-content">
            <div class="setting-wait"><i class="iconfont icon-nanguo"></i> 待配置</div>
            <span class="btn btn-sm setting-name hide"></span>
            <span class="btn btn-sm setting-env hide"></span>
          </div>
        </div>
        <div class="group">
          <ul id="group-tab">
            <li><a href="#template" data-toggle="tab">模版</a></li>
          </ul>
          <div class="tab-content">
            <div class="tab-pane fade in active" id="template">
              <ul>
                <li>
                  <div style="line-height: 50px;padding: 0;margin: 0 auto;color: #A8A8A8;padding-left: 70px;"><i class="iconfont icon-nanguo"></i> 暂无数据</div>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
      <div class="col-sm-9 case-right">
        <div class="detail-header">
          <div class="detail-header-left"><span class="detail-group-name"></span></div>
          <div class="detail-header-right">
            <span class="cursor import-case"><i class="iconfont icon-add" style="font-size: 12px;"></i>导入用例</span>
            <span class="del-group cursor"><i class="iconfont icon-shanchu" style="font-size: 12px;"></i>删除</span>
          </div>
        </div>
        <div class="detail-content clearfix">
          <div class="detail-left">
            <ul>
              <div style="line-height: 50px;padding: 0;margin: 0 auto;color: #A8A8A8;padding-left: 50px;"><i class="iconfont icon-nanguo"></i>  您还没有选择组，或者所选组暂无数据~</div>
            </ul>
          </div>
          <div class="detail-right">
            <div style="line-height: 50px;padding: 0;margin: 0 auto;color: #A8A8A8;padding-left: 50px;"><i class="iconfont icon-nanguo"></i>  您还没有选择用例，或者所选用例暂无数据~</div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- 用例设置 -->
<div class="modal fade" id="editSettingModal" tabindex="-1" role="dialog" aria-labelledby="editSettingModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content" style="width: 320px;margin: 0 auto;">
    <form class="edit-env-form">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
          &times;
        </button>
        <h4 class="modal-title">
          设置
        </h4>
      </div>
      <div class="modal-body">
        <div class="form-group">
          <label>项目：</label>
          <select class="form-control" name="id" id="setting-name">
          </select>
        </div>
        <div class="form-group">
          <label>环境：</label>
          <select class="form-control" name="env" id="setting-env">
          </select>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-sm btn-default" data-dismiss="modal">关闭
        </button>
        <button type="submit" class="btn btn-sm btn-primary">
          提交
        </button>
      </div>
    </form>
    </div><!-- /.modal-content -->
  </div><!-- /.modal -->
</div>

<!-- 导入api文件 -->
<div class="modal fade" id="importCaseModal" tabindex="-1" role="dialog" aria-labelledby="importCaseModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content" style="width: 360px; margin: 0 auto;">
    <form action="/api/cases/import-data" method="post" enctype="multipart/form-data">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
          &times;
        </button>
        <h4 class="modal-title" id="importCaseModalLabel">
          导入用例数据
        </h4>
      </div>
      <div class="modal-body">
        <input type="hidden" id="importCaseDataId" name="mid" />
         JSON文件：<input type="file" name="file" />
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-sm btn-default" data-dismiss="modal">关闭</button>
        <button type="submit" class="btn btn-sm btn-primary">导入</button>
      </div>
    </form>
    </div><!-- /.modal-content -->
  </div><!-- /.modal -->
</div>
<script type="text/javascript" src="/javascripts/case.js"></script>
